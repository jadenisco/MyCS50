{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/project3",
            "program": "manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "JS",
            "program": "${workspaceFolder}/project3/mail/static/mail/scratch.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "chrome",
            "name": "Open Link 127.0.0.1:8000/",
            "request": "launch",
            "url": "http://127.0.0.1:8000/"
        },
        {
            "type": "chrome",
            "name": "Open Link 127.0.0.1:8000/scratch",
            "request": "launch",
            "url": "http://127.0.0.1:8000/scratch/0"
        },
        {
            "type": "chrome",
            "name": "Scratch.html",
            "request": "launch",
            "file": "${workspaceFolder}/project3/mail/templates/mail/scratch.html",
        }
    ]
}